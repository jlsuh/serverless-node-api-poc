{
  "name": "serverless-node-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --ignore ./s3-local/* --exec sls offline start",
    "initDb": "npx sls invoke local -f initDb",
    "start": "sls offline start"
  },
  "nodemonConfig": {
    "events": {
      "start": "clear"
    }
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "serverless-offline": "^13.3.0",
    "serverless-offline-sqs": "^8.0.0",
    "serverless-offline-sqs-dlq": "^1.5.2",
    "serverless-s3-local": "^0.8.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.460.0",
    "@aws-sdk/client-sqs": "^3.460.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0"
  }
}
